package model;
import org.junit.Test;

public class Client {
	
	// Attributes
	
	private String name;
	
	// Constructor
	
	public Client(String name) {
		super();
		this.name = name;
	}
	
	// Getters and Setters

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
}